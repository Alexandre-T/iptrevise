{
    "name": "alexandre-t/iptrevise",
    "type": "project",
    "license": "proprietary",
    "description": "Gestion des adresses IP.",
    "authors": [
        {
            "name": "Alexandre Tranchant",
            "email": "Alexandre.Tranchant@gmail.com",
            "homepage": "https://github.com/Alexandre-T",
            "role": "Full-Stack Developer"
        }
    ],
    "support": {
        "email": "Alexandre.Tranchant@gmail.com",
        "issues": "https://github.com/Alexandre-T/iptrevise/issues",
        "source": "https://github.com/Alexandre-T/iptrevise"
    },
    "require": {
        "php": "^7.1.3|^8",
        "alexandret/phpcas-guard-bundle": "^0.2",
        "beberlei/doctrineextensions": "^1.0",
        "codeception/c3": "^2.0",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "knplabs/knp-menu-bundle": "^2.1",
        "knplabs/knp-paginator-bundle": "^2.6",
        "mopa/bootstrap-bundle": "^3.2",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "sensiolabs/security-checker": "^5.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/security-bundle": "^3.3",
        "symfony/templating": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/yaml": "^3.3",
        "twig/extensions": "^1.5",
        "ext-gd": "*"
    },
    "require-dev": {
        "codeception/codeception": "^2.3",
        "phpunit/php-code-coverage": "^4.0",
        "stecman/symfony-console-completion": "^0.8.0",
        "symfony/browser-kit": "^3.3",
        "symfony/debug-bundle": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security:check --end-point=https://security.symfony.com/check_lock": "symfony-cmd",
            "cache:clear --no-warmup": "symfony-cmd",
            "cache:warmup": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BRM57HR6VDSC2C4R11Q4XDPE",
            "allow-contrib": "true"
        }
    }
}
